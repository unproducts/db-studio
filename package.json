{
  "name": "db-studio",
  "version": "0.0.0",
  "description": "Lightweight server that exposes your database over HTTP API. Ships with a minimal UI and CLI. Supports SQLite, PostgreSQL, and MySQL.",
  "repository": "unproducts/db-studio",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.mjs"
  },
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "dist-ui"
  ],
  "bin": {
    "nuxtup": "./dist/cli.mjs"
  },
  "scripts": {
    "build": "obuild",
    "dev": "jiti src/cli.ts",
    "lint": "eslint . && prettier -c .",
    "lint:fix": "automd && eslint . --fix && prettier -w .",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "automd": "^0.4.0",
    "changelogen": "^0.6.2",
    "eslint": "^9.33.0",
    "eslint-config-unjs": "^0.5.0",
    "jiti": "^2.6.1",
    "obuild": "^0.2.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.15.0",
  "peerDependencies": {
    "node-sqlite": "0.0.2-security"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "db0": "^0.3.4",
    "h3": "2.0.1-rc.6",
    "srvx": "^0.9.8",
    "ufo": "^1.6.1"
  }
}
