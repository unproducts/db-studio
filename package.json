{
  "name": "@unproducts/db-studio",
  "version": "0.0.1",
  "description": "Lightweight server that exposes your database over HTTP API. Ships with a minimal UI and CLI. Supports SQLite, PostgreSQL, and MySQL.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unproducts/db-studio.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.mjs"
  },
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "dist-ui"
  ],
  "bin": {
    "db-studio": "./dist/cli.mjs"
  },
  "scripts": {
    "build": "pnpm build:ui && obuild",
    "build:ui": "cd ui && pnpm build",
    "dev": "jiti src/cli.ts",
    "lint": "eslint . && prettier -c .",
    "lint:fix": "automd && eslint . --fix && prettier -w .",
    "prepack": "pnpm build",
    "release": "changelogen --release && npm publish --access public && git push --follow-tags"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "automd": "^0.4.0",
    "changelogen": "^0.6.2",
    "eslint": "^9.33.0",
    "eslint-config-unjs": "^0.5.0",
    "jiti": "^2.6.1",
    "obuild": "^0.2.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "packageManager": "pnpm@10.15.0",
  "peerDependencies": {
    "node-sqlite": "0.0.2-security"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "db0": "^0.3.4",
    "h3": "2.0.1-rc.6",
    "srvx": "^0.9.8",
    "ufo": "^1.6.1"
  }
}
